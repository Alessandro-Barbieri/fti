#!/bin/bash

setup() {
    app='local/recoverName/recoverName.exe'
    head=0
    keep=0
    param_register 'iolib' 'level' 'diffsize'
}

runtest() {
    app_run_success $app $itf_cfgfile 1 $level 1
    app_run_timeout $app $itf_cfgfile 0 $level $diffsize
    assert_equals $? 0 "recoverName.exe returned with output: $?"
}

teardown() {
    unset app head keep
}
