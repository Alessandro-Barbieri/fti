#!/bin/bash

setup() {
    head=0
    keep=0
    param_register 'iolib' 'level'
}

runtest() {
    local app='@CMAKE_CURRENT_BINARY_DIR@/recoverVar.exe'
    app_run_success $app $itf_cfgfile 1 $level 1
    app_run_timeout $app $itf_cfgfile 0 $level 1
    assert_equals $? 0 "recoverVar.exe failed with output: $?"
}

teardown() {
    unset head keep
}
